<div class="col-md-12">
  <div>
    <%= link_to 'Categories', admin_categories_path %> |
    <%= link_to 'Users', admin_users_path %>
  </div>
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Role</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <th scope="row"><%= user.name %></th>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

</div>