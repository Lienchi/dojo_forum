<div class="col-md-12">
  <div>
    <%= link_to 'Categories', admin_categories_path %> |
    <%= link_to 'Users', admin_users_path %>
  </div>
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Name</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @categories.each do |category| %>
      <tr>
        <th scope="row"><%= category.name %></th>
        <td></td>
      </tr>
      <% end %>
    </tbody>
  </table>

</div>